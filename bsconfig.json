{
  "name": "bamboo",
  "version": "0.0.02",
  "bsc-flags": "-w -27 -g",
  "warnings": {
    "number": "-40+6+7-26-27+32..39-28-44+45",
    "error": "+8"
  },
  "bs-dependencies": [
    "bs-bn.js"
  ],
  "ocamlfind-dependencies": [
    "batteries",
    "cryptokit",
    "hex",
    "ppx_deriving_rpc",
    "menhirlib"
  ],
  "generators": [
    {
      "name": "ocamllex",
      "command": "ocamllex $in"
    },
    {
      "name": "menhir",
      "command": "menhir --table $in"
    }
  ],
  "sources": {
    "dir": "src",
    "subdirs": [
      "./ast",
      "./basics",
      "./codegen",
      "./cross-platform",
      "./exec",
      "./lib",
      "./parse"
    ],
    "generators": [
      {
        "name": "ocamllex",
        "edge": ["parse/lexer.ml", ":", "parse/lexer.mll"]
      },
      {
        "name": "menhir",
        "edge": ["parse/parser.ml", "parse/parser.mli", ":", "parse/parser.mly"]
      }
    ]
  },
  "entries": [
    {
      "backend": "native",
      "main-module": "Bamboo"
    },
    {
      "backend": "js",
      "main-module": "Bamboo"
    }
  ],
  "refmt": 3
}
